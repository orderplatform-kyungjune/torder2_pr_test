    "@vue/test-utils": "^2.4.5",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "mime": "^4.0.0",
    "postcss-html": "^1.4.1",
    "sass": "^1.26.5",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended-scss": "^6.0.0",
