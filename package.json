    "md5": "^2.3.0",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate": "^3.2.1",
    "socket.io-client": "2.4.0",
    "uuid": "^9.0.1",
    "vue": "3.3.0",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.0-0",
